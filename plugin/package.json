{
  "name": "payload-plugin-translator",
  "private": true,
  "version": "1.0.0-beta.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "pnpm copyfiles && pnpm build:swc && pnpm build:types",
    "build:swc": "swc ./src -d ./dist --config-file .swcrc",
    "build:types": "tsc --emitDeclarationOnly --outDir dist",
    "clean": "rimraf dist",
    "copyfiles": "copyfiles -u 1 \"src/**/*.{html,css,scss,ttf,woff,woff2,eot,svg,jpg,png,json}\" dist/",
    "prepublishOnly": "pnpm clean && pnpm build"
  },
  "peerDependencies": {
    "@payloadcms/ui": "^3.0.0-beta.19",
    "payload": "^3.0.0-beta.19",
    "@payloadcms/translations": "^3.0.0-beta.19"
  },
  "devDependencies": {
    "payload": "3.0.0-beta.19",
    "@payloadcms/ui": "3.0.0-beta.19",
    "@types/react": "18.2.64",
    "react": "18.2.0",
    "@payloadcms/translations": "3.0.0-beta.19"
  },
  "exports": {
    ".": {
      "import": "./src/index.ts",
      "require": "./src/index.ts",
      "types": "./src/index.ts"
    },
    "./resolvers/google": {
      "import": "./src/resolvers/google.ts",
      "require": "./src/resolvers/google.ts",
      "types": "./src/resolvers/google.ts"
    },
    "./resolvers/copy": {
      "import": "./src/resolvers/copy.ts",
      "require": "./src/resolvers/copy.ts",
      "types": "./src/resolvers/copy.ts"
    }
  },
  "publishConfig": {
    "exports": {
      ".": {
        "import": "./dist/index.js",
        "require": "./dist/index.js",
        "types": "./dist/index.d.ts"
      }
    },
    "main": "./dist/index.js",
    "registry": "https://registry.npmjs.org/",
    "types": "./dist/index.d.ts"
  },
  "dependencies": {
    "@radix-ui/react-dialog": "^1.0.5",
    "clsx": "^2.1.0",
    "zod": "^3.23.0"
  }
}
